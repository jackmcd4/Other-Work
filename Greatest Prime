function primeFactor(num){
    for(var i=num-1; i>0; i--){
      if(i===1){
          return i;
      }
      else if(num%i===0 && primeTest(i)===i){
          return i;
      }
    }
}
    
function primeTest(num){
      for(var j=num-1; j>0; j--){
                if(num%j===0 && j!==1){
                    return false;
                }
                else if(i%j===0 && j===1){
                    return num;
                }
        }
}

primeFactor(26);
//Returns 13
